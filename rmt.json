{
    "vcs": "git",
    "prerequisites": ["working-copy-check", "display-last-changes"],
    "pre-release-actions": [
        "composer-update",
        {
            "name": "changelog-update",
            "format": "semantic",
            "dump-commits": true
        },
        "vcs-commit"
    ],
    "version-generator": "semantic",
    "version-persister":"vcs-tag"
}